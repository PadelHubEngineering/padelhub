    "@typegoose/typegoose": "^11.1.0",
    "argon2": "^0.30.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "mongoose": "^7.1.0",
    "typescript": "^5.0.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
