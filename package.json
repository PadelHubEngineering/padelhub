    "populate": "tsc -p ./tsconfig.json && node -r dotenv/config out/populate.js",
    "aut": "ts-node-dev --respaw index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PadelHubEngineering/padelhub"
